{
  "expo": {
    "name": "Flying Birdie",
    "slug": "flying-birdie-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/bird.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/bird.png",
      "resizeMode": "contain",
      "backgroundColor": "#87CEEB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flyingbirdie.mobile",
      "infoPlist": {
        "UIRequiresFullScreen": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/bird.png",
        "backgroundColor": "#87CEEB"
      },
      "package": "com.flyingbirdie.mobile",
      "permissions": [],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/bird.png"
    },
    "extra": {
      "eas": {
        "projectId": "flying-birdie-mobile"
      }
    }
  }
}
